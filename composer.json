{
  "name": "cleentfaar/sitemap",
  "description": "Generate sitemaps with ease",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Cas Leentfaar",
      "homepage": "https://github.com/cleentfaar"
    }
  ],
  "keywords": [
    "sitemap",
    "seo"
  ],
  "require": {
    "php": "^7.1",
    "knplabs/gaufrette": "^0.3.1",
    "beberlei/assert": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7|^6.0",
    "friendsofphp/php-cs-fixer": "^2.0",
    "symfony/dependency-injection": "^3.0",
    "matthiasnoback/symfony-dependency-injection-test": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "CL\\Sitemap\\": "src/CL/Sitemap"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CL\\Sitemap\\Test\\": "tests/src/CL/Sitemap/Test",
      "CL\\Sitemap\\Tests\\": "tests/src/CL/Sitemap/Tests"
    }
  }
}
